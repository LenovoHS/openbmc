[Unit]
Description=Power Button Pressed Service
StartLimitIntervalSec=0

[Service]
Type=oneshot
ExecStart=ipmitool raw 0x0a 0x44 0x00 0x00 0x03 0x00 0x00 0x00 0x00 0x00 0x01 0x04 0x14 0xe2 0x6f 0x00 0x00 0x00 ; /usr/bin/env pwr-button.sh
SyslogIdentifier=pwr-button
